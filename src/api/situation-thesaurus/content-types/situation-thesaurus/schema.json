    "owner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "categories": {
      "type": "json"
    },
    "thoughts": {
      "type": "json"
    },
    "emotions": {
      "type": "json"
    },
    "sensations": {
      "type": "json"
    },
    "behaviors": {
      "type": "json"
    }
  }
